x = int(input("Digite um numero: "))
count = x
Numeros = "0, "
num = 0
num2 = 0
num3 = 1
while count >= 0:
    if count > 0:
        count -= 1
        Numeros += str(num3)
        Numeros += ", "
        num = num2
        num2 = num3
        num3 = num + num2
    elif count == 0:
        count -= 1
        Numeros += str(num3)
        Numeros += "."
        num = num2
        num2 = num3
        num3 = num + num2
print(Numeros)
